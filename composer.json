{
    "name": "anandpilania/laravel-country.io",
    "description": "Country.io database for laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Anand Pilania",
            "email": "pilaniaanand@gmail.com",
			"homepage": "https://kspedu.com"
        }
    ],
    "require": {
		"illuminate/console": "^8.0 || ^9.0 || ^10.0",
		"illuminate/support": "^8.0 || ^9.0 || ^10.0"
	},
	"autoload": {
		"psr-4": {
			"CountryIO\\": "src/"
		}
	},
	"extra": {
        "laravel": {
			"providers": [
                "CountryIO\\CountryIOServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
